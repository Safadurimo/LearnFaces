<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="sap.ui.demo.nav.controller.Result"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Result">
		<content>
			<Text text="Congratulation! You scored {game>/score} of {game>/number_of_questions}!" />
			<Button text="Start new Game" press="onPressStartGame"/>
			<Button text="Back to Homepage" press="onPressHomePage"/>
		</content>
	</Page>
</mvc:View>